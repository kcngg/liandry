{
  "name": "@kcngg/liandry",
  "version": "0.0.4",
  "packageManager": "pnpm@10.12.3",
  "description": "Base design system for KCN.gg applications.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/kcngg/liandry#readme",
  "bugs": {
    "url": "https://github.com/kcngg/liandry/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kcngg/liandry.git"
  },
  "author": "@npldevfr",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/liandry.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "vite build",
    "dev": "vite build --watch",
    "play": "vite",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "serve storybook-static"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "reka-ui": "^2.3.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.352",
    "@storybook/vue3-vite": "^9.0.13",
    "@sxzz/eslint-config": "^7.0.3",
    "@sxzz/prettier-config": "^2.2.1",
    "@types/node": "^22.15.32",
    "@unocss/cli": "^66.2.3",
    "@unocss/core": "^66.2.3",
    "@unocss/eslint-plugin": "^66.2.3",
    "@unocss/preset-icons": "^66.3.2",
    "@unocss/preset-uno": "^66.2.3",
    "@unocss/reset": "^66.2.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.1.1",
    "eslint": "^9.29.0",
    "eslint-plugin-storybook": "^9.0.13",
    "happy-dom": "^18.0.1",
    "lucide-vue-next": "^0.518.0",
    "prettier": "^3.5.3",
    "storybook": "^9.0.13",
    "tailwind-variants": "^1.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unocss": "^66.2.3",
    "unplugin-vue": "^6.2.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.3",
    "vue": "^3.5.16",
    "vue-tsc": "^2.2.10"
  },
  "prettier": "@sxzz/prettier-config"
}
